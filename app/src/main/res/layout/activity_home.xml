<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <FrameLayout
        android:id="@+id/HomeMainWindow"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </FrameLayout>

    <com.cleveroad.sy.cyclemenuwidget.CycleMenuWidget
        android:id="@+id/Home_MenuButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:cm_corner="right_bottom"
        app:cm_item_background_tint="@color/urbanMealsRed"
        app:cm_corner_image_src="@drawable/ic_menu"
        />
</android.support.constraint.ConstraintLayout>
